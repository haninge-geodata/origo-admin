{
  "name": "origo-admin-server",
  "version": "1.0.0",
  "description": "Serverside code in NodeJS for Haninge Admin",
  "main": "server.js",
  "type": "module",
  "dev": "ts-node-dev --pretty --respawn ./src/app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "prestart": "npm run build",
    "start": "node dist/server/src/server.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "rimraf ./dist && npx tsc && tsc-alias -p tsconfig.json && npm run generate-swagger && cp .env.template ./dist/server/.env.template && cp -R ../installation/iis/tasks ./dist/server/src/tasks/ && cp ./src/swagger_output.json ./dist/server/src/",
    "generate-initial-token": "ts-node src/scripts/generateInitialToken.ts",
    "generate-secret-key": "ts-node src/scripts/generateSecretKey.ts",
    "generate-swagger": "node --loader ts-node/esm src/generateSwagger.ts"

  },
  "keywords": [],
  "author": "",

  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/helmet": "^4.0.0",
    "@types/node": "^22.5.1",
    "@types/passport": "^1.0.16",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.23.0-alpha.20240402.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.2",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.8.0",
    "mongoose": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "openid-client": "^5.6.5",
    "passport": "^0.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-morph": "^23.0.0",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1"
  },
  "optionalDependencies": {
    "node-windows": "^0.1.14"
  }
}